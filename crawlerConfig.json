{
  "index_name": "aaron-shih",
  "start_urls": ["https://www.aaron-shih.com/"],
  "sitemap_urls": ["https://www.aaron-shih.com/sitemap.xml"],
  "sitemap_alternate_links": true,
  "rateLimit": 8,
  "maxDepth": 10,
  "selectors": {
    "lvl0": {
      "selector": "//nav[@aria-label='breadcrumb']//li[2]//text()",
      "type": "xpath",
      "global": true,
      "default_value": "Blog"
    },
    "lvl1": "main h1",
    "lvl2": "main h2",
    "lvl3": "main h3",
    "lvl4": "main h4",
    "lvl5": "main h5",
    "content": "main p, main li, main blockquote",
    "lang": {
      "selector": "/html/@lang",
      "type": "xpath",
      "global": true
    }
  },
  "selectors_exclude": [
    "aside",
    "nav",
    "footer",
    "[aria-label='breadcrumb']",
    "[data-radix-scroll-area-viewport]"
  ],
  "strip_chars": " .,;:#",
  "js_render": true,
  "custom_settings": {
    "attributesForFaceting": ["lang"]
  },
  "attributesForFaceting": ["lang"],
  "attributesToRetrieve": [
    "hierarchy",
    "content",
    "anchor",
    "url",
    "url_without_anchor",
    "type"
  ]
}
