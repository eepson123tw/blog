{
  "index_name": "AaronBlog",
  "start_urls": ["https://www.aaron-shih.com/"],
  "sitemap_urls": ["https://www.aaron-shih.com/sitemap.xml"],
  "sitemap_alternate_links": true,
  "rateLimit": 8,
  "maxDepth": 10,
  "selectors": {
    "lvl0": {
      "selector": "(//ul[contains(@class,'menu__list')]//a[contains(@class, 'menu__link menu__link--sublist menu__link--active')]/text() | //nav[contains(@class, 'navbar')]//a[contains(@class, 'navbar__link--active')]/text())[last()]",
      "type": "xpath",
      "global": true,
      "default_value": "Documentation"
    },
    "lvl1": ".content h1",
    "lvl2": ".content h2",
    "lvl3": ".content h3",
    "lvl4": ".content h4",
    "lvl5": ".content h5",
    "content": ".content p, .content li",
    "lang": {
      "selector": "/html/@lang",
      "type": "xpath",
      "global": true
    }
  },
  "selectors_exclude": [
    "aside",
    ".page-footer",
    ".next-and-prev-link",
    ".table-of-contents"
  ],
  "strip_chars": " .,;:#",
  "js_render": true,
  "custom_settings": {
    "attributesForFaceting": ["lang", "tags"]
  },
  "attributesForFaceting": ["language", "version", "type", "docusaurus_tag"],
  "attributesToRetrieve": [
    "hierarchy",
    "content",
    "anchor",
    "url",
    "url_without_anchor",
    "type"
  ]
}
